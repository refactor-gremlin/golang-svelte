{
    "name": "svelte-net-test",
    "version": "1.0.0",
    "description": "Svelte .NET Test Application",
    "scripts": {
        "dev": "concurrently \"npm run dev --prefix MySvelteApp.Client\" \"dotnet run --project MySvelteApp.Server\"",
        "docker:dev": "docker compose -f docker-compose.dev.yml up --build",
        "docker:prod": "docker compose -f docker-compose.yml up --build",
        "build": "npm run build --prefix MySvelteApp.Client && dotnet build MySvelteApp.Server",
        "format": "npm run format --prefix MySvelteApp.Client",
        "check": "npm run check --prefix MySvelteApp.Client",
        "test": "echo \"Error: no test specified\" && exit 1",
        "prepare": "husky"
    },
    "keywords": [],
    "author": "",
    "license": "ISC",
    "devDependencies": {
        "concurrently": "^8.0.0",
        "husky": "^9.1.7"
    }
}
